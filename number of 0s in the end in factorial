#include <iostream>
using namespace std;

int main() {
    
	// your code goes here
	int tc;
	cin>>tc;
	while(tc--){
	    long long num,count=0,rem;
	    //int num;
	    cin>>num;
	    while(num>0){
	        //rem=num%5;
	        num=num/5;
	        count+=num;
	        rem=num%5;
	    }
	    count+=rem;
	    cout<<count<<endl;
	}
	return 0;
}
